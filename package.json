{
  "name": "koa-react-starter-service",
  "version": "1.0.0",
  "author": "cameron@pro.com",
  "main": "dist/server.js",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "npx npm-run-all -p start:server start:app",
    "lint": "npx npm-run-all -p lint:server lint:app lint:shared lint:integration",
    "pretest": "npx check-engines",
    "test": "jest --config=jest.config.json",
    "integration": "npx npm-run-all -s integration:server integration:app",
    "package": "webpack --config webpack.config.ts",
    "prepackage": "npx check-engines",
    "postpackage": "cp package.json dist && cp package-lock.json dist && cp newrelic.js dist",
    "lint:app": "tslint -p src/app/tsconfig.json",
    "start:app": "webpack-dev-server --no-info --hot --open --history-api-fallback --config src/app/webpack.dev.ts",
    "test:app": "jest --config=src/app/jest.config.json",
    "preintegration": "npx check-engines",
    "integration:app": "start-server-and-test start:server:integration 4000 cypress:run",
    "integration:app:dev": "start-server-and-test start:server:integration 4000 cypress:open",
    "lint:server": "tslint -p src/server/tsconfig.json",
    "prestart:server": "npm run prepackage",
    "start:server": "ts-node-dev -P src/server/tsconfig.json src/server | bunyan -L -o short",
    "start:server:integration": "NODE_ENV=integration npm run package && node dist/server.js",
    "test:server": "jest --config=src/server/jest.config.json",
    "integration:server": "NODE_ENV=integration jest --config=integration/server/jest.config.json",
    "cypress:run": "npx cypress run",
    "cypress:open": "npx cypress open",
    "lint:integration": "npx npm-run-all -p lint:integration:app lint:integration:server",
    "lint:integration:app": "tslint -p integration/app/tsconfig.json",
    "lint:integration:server": "tslint -p integration/server/tsconfig.json",
    "lint:shared": "tslint -p src/shared/tsconfig.json",
    "test:shared": "jest --config=src/shared/jest.config.json"
  },
  "engines": {
    "node": "10.10.0",
    "npm": "6.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "cypress/**/*.{ts,tsx}": [
      "npm run lint:cypress"
    ],
    "src/app/**/*.{ts,tsx}": [
      "npm run lint:app"
    ],
    "src/server/**/*.{ts,tsx}": [
      "npm run lint:server"
    ],
    "src/shared/**/*.{ts,tsx}": [
      "npm run lint:shared"
    ],
    "src/integration/**/*.{ts,tsx}": [
      "npm run lint:integration"
    ],
    "src/**/*.{ts,tsx}": [
      "npm test -- --findRelatedTests"
    ]
  },
  "dependencies": {
    "@koa/cors": "2.2.2",
    "@material-ui/core": "3.1.2",
    "@material-ui/icons": "3.0.1",
    "@serviceslabs/material-ui-pro": "1.0.2",
    "axios": "0.18.0",
    "bunyan": "1.8.12",
    "classnames": "2.2.6",
    "config": "2.0.1",
    "gelf-stream": "1.1.1",
    "io-ts": "1.3.0",
    "io-ts-reporters": "0.0.21",
    "koa": "2.5.3",
    "koa-bodyparser": "4.2.1",
    "koa-connect-history-api-fallback": "0.3.1",
    "koa-mount": "4.0.0",
    "koa-newrelic": "2.0.3",
    "koa-respond": "2.1.0",
    "koa-router": "7.4.0",
    "koa-static": "5.0.0",
    "newrelic": "4.9.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-hot-loader": "4.3.11",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "typesafe-actions": "2.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-transform-classes": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@cypress/webpack-preprocessor": "3.0.0",
    "@types/bunyan": "1.8.4",
    "@types/classnames": "2.2.6",
    "@types/config": "0.0.34",
    "@types/enzyme": "3.1.14",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/gelf-stream": "file:src/server/types/gelf-stream",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/jest": "23.3.2",
    "@types/koa": "2.0.46",
    "@types/koa-bodyparser": "5.0.1",
    "@types/koa-connect-history-api-fallback": "file:src/server/types/koa-connect-history-api-fallback",
    "@types/koa-mount": "3.0.1",
    "@types/koa-router": "7.0.32",
    "@types/koa-static": "4.0.0",
    "@types/koa__cors": "2.2.3",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.8",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-redux": "6.0.9",
    "@types/react-router-dom": "4.3.1",
    "@types/redux-logger": "3.0.6",
    "@types/supertest": "2.0.6",
    "@types/webpack": "4.4.13",
    "@types/webpack-env": "1.13.6",
    "@types/webpack-node-externals": "1.6.3",
    "babel-loader": "8.0.4",
    "check-engines": "1.5.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "html-webpack-plugin": "3.2.0",
    "import-fresh": "2.0.0",
    "jest": "23.6.0",
    "npm-run-all": "4.1.3",
    "ts-jest": "23.10.3",
    "ts-loader": "5.2.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "webpack-node-externals": "1.7.2"
  },
  "optionalDependencies": {
    "cypress": "3.1.0",
    "husky": "1.1.0",
    "ts-node-dev": "1.0.0-pre.30",
    "webpack-dev-server": "3.1.9",
    "supertest": "3.3.0",
    "start-server-and-test": "1.7.3",
    "lint-staged": "7.3.0",
    "fork-ts-checker-webpack-plugin": "0.4.9"
  }
}
