{
    "displayName": "server",
    "preset": "ts-jest",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "setupFiles": ["./tests/setup.ts"],
    "testPathIgnorePatterns": [
        "./config/*",
        "./tests/setup.ts"
    ],
    "collectCoverageFrom": [
        "**/*.{ts,tsx}",
        "!**/*.d.ts",
        "!**/config/**",
        "!**/webpack.*.ts"
    ],
    "coverageDirectory": "../../coverage/server"
}
