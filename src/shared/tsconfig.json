{
    "compilerOptions": {
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "strict": true,
        "noUnusedLocals": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "lib": ["es2018", "dom"],
        "skipLibCheck": true,
        "baseUrl": "."
    },
    "include": [
        "**/*"
    ]
}
